contract TabooBuy is Initializable, ReentrancyGuardUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    struct Exchange {
        address signContractAddress;
        address userAddress;
        uint256 amountIn;
        uint256 ChainIn;
        uint256 amountOut;
        uint256 timeStamp;
    }

    string private constant name = "Taboo-buyV2";
    string private constant version = "1";

    address public owner;
    address public incommingWallet;
    address public outgoingWallet;
    address public executionWallet;
    address public signerWallet;

    IERC20 public tabooToken = IERC20(0x9abDbA20EdFbA06B782126b4D8d72A5853918FD0);

    bool public paused;

    mapping(address => bool) public isBlacklisted;
    mapping(uint256 => address) exchangeAddress;
    mapping(address => mapping(bytes32 => bool)) isValidSignature;
    mapping(address => Exchange[]) public UserData;

    modifier isBlacklist(address _user) {
        require(!isBlacklisted[_user], "BNB amount not enough");
        _;
    }

    modifier priceGreaterThanZero(uint256 _price) {
        require(_price > 0, "Price cannot be 0");
        _;
    }

    modifier onlyOwner() {
        require(owner == msg.sender, "not owner");
        _;
    }

    modifier whenNotPaused() {
        require(!paused, "contract paused");
        _;
    }

    event Blacklisted(address account, bool value);
    event coinExchange(
        address from,
        address contractAddress,
        address to,
        Exchange exchange
    );

    event claimTabooToken(
        address userAddress,
        address adminWallet,
        uint8 v,
        bytes32 r,
        bytes32 s,
        Exchange exchange
    );

    event Pausable(bool state);

    function initialize(
        address _owner,
        address _incommingWallet,
        address _outgoingWallet,
        address _signerWallet,
        address _executionWallet
    ) external initializer {
        owner = _owner;
        incommingWallet = _incommingWallet;
        outgoingWallet = _outgoingWallet;
        signerWallet = _signerWallet;
        executionWallet = _executionWallet;
    }

    function blacklistMalicious(address account, bool value)
        external
        onlyOwner
        nonReentrant
    {
        isBlacklisted[account] = value;
        emit Blacklisted(account, value);
    }

    function setOwner(
        address _owner,
        address _incommingWallet,
        address _outgoingWallet,
        address _signerWallet,
        address _executionWallet
        ) external nonReentrant onlyOwner {
        owner = _owner;
        incommingWallet = _incommingWallet;
        outgoingWallet = _outgoingWallet;
        signerWallet = _signerWallet;
        executionWallet = _executionWallet;
    }

    function setExchangeAddress(uint256 _type, address _contractAddress)
        external
        nonReentrant
        onlyOwner
    {
        exchangeAddress[_type] = _contractAddress;
    }

