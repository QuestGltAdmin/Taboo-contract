        require(
            msg.sender == executionWallet,
            "only execution wallet can call this buy"
        );
        address signer = executeSetIfSignatureMatch(
            exchangeAddress[exchange.ChainIn],
            exchange.userAddress,
            exchange.amountIn,
            exchange.ChainIn,
            exchange.amountOut,
            exchange.timeStamp,
            v,
            r,
            s
        );
        require(signer == signerWallet, "Invalid Signature");
        
        require(isValidSignature[exchange.userAddress][r] == false, "Already executed");
        
        isValidSignature[exchange.userAddress][r] = true;

        tabooTransaction(exchange);

        emit claimTabooToken(
            exchange.userAddress,
            outgoingWallet,
            v,
            r,
            s,
            exchange
        );
    }

    function executeSetIfSignatureMatch(
        address signContractAddress,
        address userAddress,
        uint256 amountIn,
        uint256 ChainIn,
        uint256 amountOut,
        uint256 timeStamp,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal pure returns (address) {
        bytes32 eip712DomainHash = keccak256(
            abi.encode(
                keccak256("EIP712Domain(string name,string version)"),
                keccak256(bytes("Taboo-buyV2")),
                keccak256(bytes("1"))
            )
        );

        bytes32 hashStruct = keccak256(
            abi.encode(
                keccak256(
                    "Permit(address signContractAddress,address userAddress,uint256 amountIn,uint256 ChainIn,uint256 amountOut,uint256 timeStamp)"
                ),
                signContractAddress,
                userAddress,
                amountIn,
                ChainIn,
                amountOut,
                timeStamp
            )
        );

        bytes32 hash = keccak256(
            abi.encodePacked("\x19\x01", eip712DomainHash, hashStruct)
        );
        address signer = ecrecover(hash, v, r, s);
        return signer;
    }

    receive() external payable {}
}